{
    "name": "pistol88/yii2-shop",
    "description": "yii2-shop it's a module for site micro-shop backend",
    "keywords": ["yii2", "yii 2", "application", "backend", "crud", "shop", "module", "pistol88"],
    "homepage": "https://github.com/pistol88/yii2-micro-shop",
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "support": {
        "source": "https://github.com/pistol88/yii2-micro-shop"
    },
    "authors": [
        {
            "name": "Ilya Pistol",
            "email": "pistol88@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": "*",
        "yiisoft/yii2-bootstrap": "*",
        "2amigos/yii2-grid-view-library": "*",
        "pistol88/yii2-gallery": "*",
        "kartik-v/yii2-widget-select2": "*",
        "trntv/yii2-file-kit": "@stable",
        "league/flysystem": "*"
    },
    "require-dev": {
        "yiisoft/yii2-debug": "*",
        "yiisoft/yii2-gii": "*"
    },
    "config": {
        "process-timeout": 1800
    },
    "autoload": {
        "psr-4": {
            "pistol88\\microshop\\": ""
        }
    },
    "extra": {
        "bootstrap": "pistol88\\microshop\\Bootstrap"
    }
}
